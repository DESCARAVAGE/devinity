{% extends 'base.html.twig' %}

{% block title %}New Project
{% endblock %}

{% block body %}
	<main class="d-flex flex-column align-items-center new-idea mt-5">
		<h1 class="text-white my-4">Create new Project</h1>
		<div class="d-flex justify-content-end mb-5">
			<a href="{{ path('home_index') }}" class="btn btn-primary">Back</a>
		</div>
		<div class="d-flex flex-column col-lg-6">
			{{ form_start(form) }}
			{{ form_widget(form) }}
			<button class="btn">{{ button_label|default('Save') }}</button>
			{{ form_end(form) }}


		</div>
	</main>
{% endblock %}
